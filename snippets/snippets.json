{
    "snippetSingleLine": {
        "prefix": "snippet-single",
        "body": [
            "\"${1:name}\": {",
            "\t\"prefix\": \"${2:prefix}\",",
            "\t\"body\": \"$0\",",
            "\t\"description\": \"${3:description}\"",
            "}"
        ],
        "description": "Create Custom or User snippet with single line body"
    },
    "snippetMultiLine": {
        "prefix": "snippet-multi",
        "body": [
            "\"${1:name}\": {",
            "\t\"prefix\": \"${2:prefix}\",",
            "\t\"body\": [",
            "\t\t$0",
            "\t],",
            "\t\"description\": \"${3:description}\"",
            "}"
        ],
        "description": "Create Custom or User snippet with multiple line body"
    },
    "snippetMultiLineBody": {
        "prefix": "snippet-multi-body",
        "body": "\"${1:body}\"$0",
        "description": "Add snippet body to multi-line Custom or User snippet"
    }
}